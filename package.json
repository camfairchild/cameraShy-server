{
	"name": "camerashy",
	"version": "0.0.1",
	"description": "IOS App for UoftHacks",
	"main": "dist/server.js",
	"scripts": {
		"start": "node dist/server.js",
		"build": "tsc",
		"watch-ts": "tsc -w",
		"watch-node": "nodemon dist/server.js",
		"watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
		"test": "mocha -r ts-node/register src/**/*.spec.ts"
	},
	"dependencies": {
		"@azure/cognitiveservices-face": "^4.2.0",
		"@types/express": "^4.17.13",
		"@types/multer": "^1.4.7",
		"@types/node": "^16.10.2",
		"axios": "^0.22.0",
		"concurrently": "^6.2.2",
		"connect": "^3.7.0",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"fs": "^0.0.1-security",
		"google-auth-library": "^7.11.0",
		"http-errors": "^1.8.0",
		"https": "^1.0.0",
		"lodash": "^4.17.21",
		"mongoose": "6.0.8",
		"multer": "^1.4.3",
		"nodemon": "^2.0.13",
		"path": "0.12.7",
		"pug": "^3.0.2",
		"socket.io": "^4.2.0",
		"tsc": "^2.0.3",
		"typescript": "^4.4.3",
		"yarn": "^1.22.15"
	},
	"engines": {
		"node": ">=16.10.0"
	},
	"keywords": [
		"node",
		"express"
	],
	"license": "MIT",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.32.0",
		"@typescript-eslint/parser": "^4.32.0",
		"eslint": "^7.32.0",
		"tslint": "^6.1.3"
	}
}
