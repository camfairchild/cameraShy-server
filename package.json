{
	"name": "camerashy",
	"version": "0.0.1",
	"description": "IOS App for UoftHacks",
	"main": "dist/server.js",
	"scripts": {
		"start": "node dist/server.js",
		"build": "tsc",
		"watch-ts": "tsc -w",
		"watch-node": "nodemon dist/server.js",
		"watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
		"test": "mocha -r ts-node/register src/**/*.spec.ts"
	},
	"dependencies": {
		"@azure/cognitiveservices-face": "^4.1.0",
		"@types/multer": "^1.4.5",
		"@types/socket.io": "^2.1.13",
		"axios": "^0.21.1",
		"concurrently": "^6.0.0",
		"connect": "^3.7.0",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"fs": "^0.0.1-security",
		"http-errors": "^1.8.0",
		"https": "^1.0.0",
		"ip": "^1.1.5",
		"mongoose": "5.11.15",
		"multer": "^1.4.2",
		"nodemon": "^2.0.7",
		"path": "0.12.7",
		"pug": "^3.0.2",
		"socket.io": "^3.1.2",
		"tsc": "^1.20150623.0",
		"yarn": "^1.22.10"
	},
	"engines": {
		"node": "^14.15.5"
	},
	"keywords": [
		"node",
		"express"
	],
	"license": "MIT",
	"devDependencies": {
		"@types/express": "^4.17.11",
		"@types/node": "^14.14.31",
		"@typescript-eslint/eslint-plugin": "^4.16.1",
		"@typescript-eslint/parser": "^4.16.1",
		"eslint": "^7.21.0",
		"tslint": "^5.20.1",
		"typescript": "^3.9.9"
	}
}
